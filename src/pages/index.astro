---
import BaseLayout from "../layouts/BaseLayout.astro";
import homePage from "../data/homePage";
import HomeHeader from "../components/HomeHeader.astro";
import Welcome from "../components/Welcome.astro";
import FleetComp from "../components/FleetComp.astro";
import fleetCompData from "../data/fleetComp";
import CTAColor from "../components/CTAColor.astro";
import CTAFlight from "../data/CTAFlight.js";
import Financing from "../components/Financing.astro";
import FinancingData from "../data/financingData.js";
import Carousel from "../components/Carousel.astro";
import Testimonials from "../components/Testimonials.astro";
import testimonialsData from "../data/testimonialsData.js";
import Form from "../components/Form.astro";
import indexFormData from "../data/indexFormData.js";


const { data } = homePage;
---

<BaseLayout
  siteTitle={data.pageTitle}
  siteDescription={data.pageDescription}
  keywords={data.pageKeywords}
>
  <HomeHeader />
  <Welcome />
  <FleetComp content={fleetCompData}/>
  <Financing content={FinancingData} />
  <Carousel />
  <Testimonials content={testimonialsData} />
  <CTAColor content={CTAFlight}/>
  <Form content={indexFormData}/>
</BaseLayout>
